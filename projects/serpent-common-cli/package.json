{
  "name": "@serpent/common-cli",
  "version": "1.0.3",
  "description": "通用的 cli 相关的函数",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "keywords": [
    "cli",
    "common",
    "serpent"
  ],
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "build:tsc": "tsc",
    "build": "rollup -m -c config/rollup.config.js",
    "build:watch": "npm run build -- -w",
    "prepare": "npm run lint && npm run test && npm run build",
    "preversion": "npm run prepare",
    "postversion": "npm publish && git push"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=8.9.0",
    "npm": ">=6.0.0"
  },
  "author": "Mora <qiuzhongleiabc@126.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/qiu8310/serpent"
  },
  "license": "MIT",
  "devDependencies": {
    "@serpent/dev-kits": "^1.0.3",
    "@serpent/rollup-kits": "^1.0.3",
    "debug": "~4.1.1",
    "dotenv": "~8.2.0"
  },
  "dependencies": {
    "@types/debug": "~4.1.5",
    "@types/semver": "~7.3.3",
    "axios": "~0.20.0",
    "mora-scripts": "^1.7.3",
    "semver": "~7.3.2"
  },
  "jest": {
    "preset": "./node_modules/@serpent/dev-kits/jest.config.js",
    "testEnvironment": "node"
  },
  "eslintConfig": {
    "env": {
      "browser": false,
      "node": true
    },
    "extends": [
      "./node_modules/@serpent/dev-kits/.eslintrc"
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "project": "./tsconfig.json"
    }
  },
  "eslintIgnore": [
    "dist",
    "lib",
    "coverage"
  ],
  "prettier": "@serpent/dev-kits"
}

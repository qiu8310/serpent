{
  "name": "serpent",
  "version": "0.0.0",
  "description": "a mono repository for serpent",
  "private": true,
  "keywords": [
    "serpent"
  ],
  "scripts": {
    "sync": "node scripts/sync_versions",
    "b:1": "cd projects/serpent-cli && npm i",
    "b:2": "cd projects/serpent-dev-kits && npm i",
    "b:3": "cd projects/serpent-rollup-kits && npm i",
    "b:4": "cd projects/serpent-react-kits && npm i",
    "b:5": "cd projects/serpent-react-admin-kits && npm i",
    "b:6": "cd projects/serpent-template && npm i",
    "b:7": "cd projects/serpent-common-react && npm i",
    "b:8": "cd projects/serpent-common-cli && npm i",
    "bootstrap": "npm run b:1 && npm run b:2 && npm run b:3 && npm run b:4 && npm run b:5 && npm run b:6 && npm run b:7 && npm run b:8",
    "test": "lerna run test --ignore @serpent/cli",
    "postinstall": "npm run bootstrap"
  },
  "license": "GPL-2.0",
  "author": "Mora <qiuzhongleiabc@126.com> (https://github.com/qiu8310)",
  "repository": {
    "type": "git",
    "url": "https://github.com/qiu8310/serpent"
  },
  "devDependencies": {
    "dot-template-types": "0.0.9",
    "lerna": "^3.13.4"
  }
}
